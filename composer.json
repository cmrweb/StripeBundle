{
    "name": "cmrweb/stripe-bundle",
    "description": "Symfony Stripe helper bundle",
    "type": "symfony-bundle",
    "keywords": ["symfony-ux"],
    "require": {
        "stripe/stripe-php": "^19.0",
        "symfony/string": "^7.3",
        "symfony/form": "^7.3",
        "doctrine/collections": "^2.4",
        "symfony/flex": "^2.10",
        "symfony/http-kernel": "^8.0",
        "symfony/ux-live-component": "^2.31",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0",
        "symfony/serializer": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/asset-mapper": "^8.0",
        "symfony/asset": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/yaml": "^8.0",
        "symfony/console": "^8.0",
        "symfony/runtime": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cmrweb\\StripeBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "cmrweb",
            "email": "tykrastagames@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/maker-bundle": "^1.65"
    }
}
